{
    "name": "practices-ui",
    "$schema": "../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "practices-ui/src",
    "prefix": "pui",
    "targets": {
        "build": {
            "executor": "@nx/angular:package",
            "options": {
                "tsConfig": "practices-ui/tsconfig.lib.json",
                "project": "practices-ui/ng-package.json"
            },
            "configurations": {
                "production": {
                    "tsConfig": "practices-ui/tsconfig.lib.prod.json"
                }
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "practices-ui/jest.config.ts"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "options": {
                "lintFilePatterns": [
                    "practices-ui/**/*.ts",
                    "practices-ui/package.json"
                ]
            }
        }
    }
}
