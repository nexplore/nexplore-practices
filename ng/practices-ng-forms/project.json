{
    "name": "practices-ng-forms",
    "$schema": "../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "practices-ng-forms/src",
    "prefix": "pui",
    "projectType": "library",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@nx/angular:package",
            "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
            "options": {
                "project": "practices-ng-forms/ng-package.json"
            },
            "configurations": {
                "production": {
                    "tsConfig": "practices-ng-forms/tsconfig.lib.prod.json"
                },
                "development": {
                    "tsConfig": "practices-ng-forms/tsconfig.lib.json"
                }
            },
            "defaultConfiguration": "production"
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "practices-ng-forms/jest.config.ts"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "options": {
                "lintFilePatterns": [
                    "practices-ui/**/*.ts",
                    "practices-ui/package.json"
                ]
            }
        }
    }
}
